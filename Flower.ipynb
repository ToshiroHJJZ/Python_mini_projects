    "from turtle import *\n",
    "import colorsys\n",
    "\n",
    "tracer(100)\n",
    "bgcolor('black')\n",
    "pensize(20)\n",
    "h=0.6\n",
    "up()\n",
    "goto(-150,0)\n",
    "down()\n",
    "\n",
    "for i in range(580):\n",
    "    c=colorsys.hsv_to_rgb(h,1,1)\n",
    "    h+=0.001\n",
    "    color(c)\n",
    "    up(), circle(i,20)\n",
    "    down(), circle(65,145)\n",
    "    lt(141)\n",
    "\n",
    "done()"
   ]
  }
